<?xml version='1.0' encoding='utf-8'?>
<widget id="es.amplya.clientesat4" version="0.3.24" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AT4grupo</name>
    <description>
        AT4grupo - Notificaciones clientes
    </description>
    <author email="mas_info@amplya.es" href="https://amplya.es/">
        Amplya, S.L.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#013760" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
</widget>
